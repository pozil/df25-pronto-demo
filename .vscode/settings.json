{
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/.sf": true,
    "**/.sfdx": true,
    "**/.localdevserver": true
  },
  "salesforce.einsteinForDevelopers.advanced": {
    "enableAgenticChat": true,
    "internalDevMMS": true, // deprecated, use setting below instead but you can leave it in
    "internalModelPicker": true,
    "salesforce.einsteinForDevelopers.enableDebugLogging": false,
    "codeAnalyzer.enableV5": true,
    "trace": {
        "request": false,
        "response": false
    }
  },
  "codeBuilder.showWelcomePage": true
}
